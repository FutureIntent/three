{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "@components/*" : [
                "./src/components/*"
            ],
            "@contexts/*" : [
                "./src/contexts/*"
            ],
            "@functions/*" : [
                "./src/functions/*"
            ],
            "@hooks/*" : [
                "./src/hooks/*"
            ],
            "@styles/*" : [
                "./src/styles/*"
            ],
            "@tests/*" : [
                "./src/tests/*"
            ],
        }
    }
}